{
  "name": "configs-in-package",
  "version": "0.0.15",
  "private": true,
  "dependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-use-babel-config": "^0.0.10",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-require": "^3.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.2"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "rescripts": [
    [
      "use-babel-config",
      {
        "presets": [
          "react-app"
        ],
        "plugins": [
          "react-require",
          [
            "module-resolver",
            {
              "root": ".",
              "alias": {
                "~": "./src"
              }
            }
          ]
        ]
      }
    ],
    [
      "use-eslint-config",
      {
        "extends": "react-app",
        "rules": {
          "react/react-in-jsx-scope": 0
        }
      }
    ]
  ]
}
